<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp"/>
            <solid android:color="@color/blue_grey200"/>
            <stroke android:color="@color/blue_grey100"
                android:width="1dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp"/>
            <solid android:color="@color/blue_grey100"/>
            <stroke android:color="@color/blue_grey50"
                android:width="1dp"/>



        </shape>
    </item>
</selector>